
pub fn add(ref dest: $T[], item: $T)
{
    // TODO bsearch instead
    for (mut i = 0; i < dest.len; i++)
    {
        if (dest[i] >= item)
        {
            if (dest[i] != item)
                dest.insert(i, item);

            return;
        }
    }

    dest.push(item);
}

pub fn add_set(ref dest: $T[], src: $T[])
{
    for (mut i = 0; i < src.len; i++)
        dest.add(src[i]);
}
