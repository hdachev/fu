
pub struct TestDiffs
{
    keys: string[];
    vals: string[];
};

pub fn serialize(diffs: TestDiffs)
{
    mut res: string;

    let keys = diffs.keys;
    let vals = diffs.vals;
    for (mut i = 0; i < keys.len; i++)
        res ~= keys[i] ~ "\n-----\n" ~ vals[i] ~ "\n-----\n";

    return res;
}

pub fn set_next(ref diffs: TestDiffs, key: string, val: string)
{
    diffs.keys ~= key;
    diffs.vals ~= val;
}
