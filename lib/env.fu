
fn get(mut name: string): string
{
    // C-str ok.
    name ~= '\0';

    pragma include(
        "<stdlib.h>");

    pragma emit(
    `
        const char* value = getenv(`name`.data());
        return fu_STRLIT { value, value ? (fu::i) strlen(value) : 0 };
    `);

    // Redundant.
    return name;
}
