
fn get(mut name: string): string
{
    compiler include(
        "<stdlib.h>",
        "<fu/vec/c_str.h>");

    compiler emit(`

        const char* value = getenv(FU_TEMP_CSTR(`, name, `));
        return fu_STRLIT { value, value ? (fu::i) strlen(value) : 0 };

    `);

    // Redundant.
    return name;
}
